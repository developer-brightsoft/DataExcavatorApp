<UserControl x:Class="DEClientInterface.UIControls.DEDataRowInnerItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DEClientInterface.UIControls" xmlns:fa5="http://schemas.fontawesome.com/icons/"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Margin="5,0,5,5">        <StackPanel Orientation="Vertical">            <Label Name="FoundItemNumberLabel" FontSize="14" FontWeight="Bold">Item #1</Label>            <TabControl>                <TabItem>                    <HeaderedContentControl.Header>                        <TextBlock FontSize="16" Margin="5,0">HTML</TextBlock>                    </HeaderedContentControl.Header>                    <Grid>                        <Grid.ColumnDefinitions>                            <ColumnDefinition Width="1*" />                        </Grid.ColumnDefinitions>                        <RichTextBox Name="DEInnerHTMLResultsTextArea" Height="94" FontSize="16" Padding="7,3" />                        <Button Name="DEExpandCollapseHTMLResultsArea" VerticalAlignment="Top" HorizontalAlignment="Right" Width="32" Height="30" Grid.Row="0" Grid.Column="1" Style="{StaticResource HelpIconRectangle}" Click="DEExpandCollapseHTMLResultsArea_Click">                            <fa5:SvgAwesome Icon="Regular_Eye" Foreground="#2b71bc" VerticalAlignment="Center" HorizontalAlignment="Center" />                        </Button>                    </Grid>                </TabItem>                <TabItem>                    <HeaderedContentControl.Header>                        <TextBlock FontSize="16" Margin="5,0">Text</TextBlock>                    </HeaderedContentControl.Header>                    <Grid>                        <Grid.ColumnDefinitions>                            <ColumnDefinition Width="1*" />                        </Grid.ColumnDefinitions>                        <RichTextBox Name="DEInnerTextResultsTextArea" Height="94" FontSize="16" Padding="7,3" />                        <Button Name="DEExpandCollapseInnerTextArea" VerticalAlignment="Top" HorizontalAlignment="Right" Width="32" Height="30" Grid.Row="0" Grid.Column="1" Style="{StaticResource HelpIconRectangle}" Click="DEExpandCollapseInnerTextArea_Click">                            <fa5:SvgAwesome Icon="Regular_Eye" Foreground="#2b71bc" VerticalAlignment="Center" HorizontalAlignment="Center" />                        </Button>                    </Grid>                </TabItem>                <TabItem Name="DEDataAttributesTab">                    <HeaderedContentControl.Header>                        <TextBlock FontSize="16" Margin="5,0">Attributes</TextBlock>                    </HeaderedContentControl.Header>                    <Grid>                        <Grid.ColumnDefinitions>                            <ColumnDefinition Width="1*" />                        </Grid.ColumnDefinitions>                        <RichTextBox Name="DEAttributesResultsTextArea" Height="94" FontSize="16" Padding="7,3" />                        <Button Name="DEExpandCollapseAttributesTextArea" VerticalAlignment="Top" HorizontalAlignment="Right" Width="32" Height="30" Grid.Row="0" Grid.Column="1" Style="{StaticResource HelpIconRectangle}" Click="DEExpandCollapseAttributesTextArea_Click">                            <fa5:SvgAwesome Icon="Regular_Eye" Foreground="#2b71bc" VerticalAlignment="Center" HorizontalAlignment="Center" />                        </Button>                    </Grid>                </TabItem>                <TabItem Name="DEDataMediaTab">                    <HeaderedContentControl.Header>                        <TextBlock FontSize="16" Margin="5,0">Media</TextBlock>                    </HeaderedContentControl.Header>                    <Grid>                        <ScrollViewer Name="NodeInnerMediaScrollViewer" Visibility="Collapsed" Grid.Row="2" Height="250" Background="#ebf3ff" Grid.Column="1" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Hidden">                            <StackPanel Name="NodeInnerMediaScrollViewerContentPanel" Orientation="Horizontal" />                        </ScrollViewer>                        <Label Name="LabelNoAssociatedMedia" FontSize="16">No associated media</Label>                    </Grid>                </TabItem>            </TabControl>        </StackPanel>    </Grid></UserControl>
