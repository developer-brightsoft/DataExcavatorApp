<Window x:Class="DEClientInterface.UIWindows.DEAddLinksToCrawlingResults"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DEClientInterface.UIWindows" 
        Title="PROJECTNAME ? Results of links addition" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*" />
            <RowDefinition Height="40" />
        </Grid.RowDefinitions>
        <TextBox Name="AddLinksToCrawlingContentResults" FontSize="14" Padding="5" Margin="5" TextWrapping="Wrap" AcceptsReturn="True" VerticalScrollBarVisibility="Visible" />
        <StackPanel Grid.Row="2" Orientation="Horizontal" Margin="0,0,5,5" HorizontalAlignment="Center">
            <Button Name="OkButton" Padding="15,0" FontSize="16" Click="OkButton_Click">OK</Button>
        </StackPanel>
    </Grid>
</Window>
