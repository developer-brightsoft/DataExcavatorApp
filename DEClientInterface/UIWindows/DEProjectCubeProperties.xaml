                        <TextBlock FontSize="16">Export</TextBlock>
                    </StackPanel>
                </Button>
                <Button Name="SaveProjectPropertiesButton" Padding="15,0" Margin="10,0,0,0" Click="SaveProjectPropertiesButton_Click">
                    <StackPanel Orientation="Horizontal">
                        <fa5:SvgAwesome Icon="Regular_Save" Foreground="#2b71bc" VerticalAlignment="Center" HorizontalAlignment="Center" Width="16" Height="16" />
                        <TextBlock FontSize="16" VerticalAlignment="Center" Margin="7,0,0,0">Save</TextBlock>
                    </StackPanel>
                </Button>
            </StackPanel>
        </Grid>
        <Border Name="ProjectPropertiesShadowOverlay" Grid.RowSpan="2" Background="#cccccc" Visibility="Hidden" Style="{StaticResource VisibleAnimation}" />
    </Grid>
</Window>
